[Unit]
Description=Airflow webserver daemon

[Service]
EnvironmentFile="PATH=/home/hadoop/miniconda3/envs/airflow_env/bin"
User=hadoop
#Group=airflow
Type=simple
ExecStart=/home/hadoop/miniconda3/envs/airflow_env/bin/airflow webserver
Restart=on-failure
RestartSec=5s
PrivateTmp=true

[Install]
WantedBy=multi-user.target
